    "import h5py\n",
    "import numpy as np\n",
    "import os\n",
    "\n",
    "root_path = '/workspace/data/GOPRO/test'\n",
    "h5_folder = os.listdir(root_path)\n",
    "h5_folder.sort()\n",
    "total_rmse = 0\n",
    "threshold = 0.05\n",
    "\n",
    "gen_event_mother_path = '/workspace/FFTformer/results/Gopro_test/visualization'\n",
    "\n",
    "for h5 in h5_folder:\n",
    "    h5_path = os.path.join(root_path, h5)\n",
    "    with h5py.File(h5_path, 'a') as f:\n",
    "        print(f\"\\n### Inspecting file: {h5} ###\")\n",
    "        # print(\"Existing keys:\", list(f.keys()))\n",
    "        scene = h5_path.split('/')[-1][:-3]\n",
    "        \n",
    "        \n",
    "        gt_event = f['voxels']\n",
    "\n",
    "        for event in gt_event:  # voxel000000000\n",
    "            event_data = gt_event[f'{event}'][:]\n",
    "            img_tag = f'image{event[8:]}'\n",
    "            \n",
    "            gen_event_path = os.path.join(gen_event_mother_path, scene, img_tag, 'out.npy')\n",
    "            gen_event_data = np.load(gen_event_path)\n",
    "\n",
    "            gen_event_data[np.abs(gen_event_data) <= threshold] = 0\n",
    "            event_data[np.abs(event_data) <= threshold] = 0\n",
    "            \n",
    "            max_val = np.max(np.abs(event_data))\n",
    "            event_data = event_data / max_val\n",
    "\n",
    "\n",
    "\n",
    "            gen_event_data = (gen_event_data + 1) / 2.0\n",
    "            gt_event_data = (event_data + 1) / 2.0\n",
    "\n",
    "            rmse = np.sqrt(np.mean((gen_event_data - gt_event_data) ** 2))\n",
    "\n",
    "            \n",
    "            total_rmse += rmse\n",
    "   \n",
    "\n",
    "print(total_rmse / 1089)"
   ]
